        "@codingame/monaco-vscode-javascript-default-extension": "^1.83.11",
        "@codingame/monaco-vscode-languages-service-override": "^1.83.11",
        "@codingame/monaco-vscode-output-service-override": "^1.83.11",
        "@codingame/monaco-vscode-textmate-service-override": "1.83.11",
        "@codingame/monaco-vscode-theme-service-override": "1.83.11",
        "@codingame/monaco-vscode-views-service-override": "^1.83.11",
        "@codingame/tsconfig": "^1.1.1",
        "@types/dockerode": "^3.3.23",
        "@types/express": "^4.17.21",
        "@types/react": "18.2.38",
