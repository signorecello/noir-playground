{
    "name": "@signorecello/noir_playground",
    "version": "0.2.7",
    "type": "module",
    "main": "src/index.ts",
    "exports": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js",
        "default": "./dist/esm/index.js"
    },
    "files": [
        "dist",
        "package.json"
    ],
