        "lint": "eslint --ext .ts,.tsx src",
        "format": "prettier --write \"src/**/*.{ts,tsx}\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "publish": "yarn build && yarn npm publish --access public",
