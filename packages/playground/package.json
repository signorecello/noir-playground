        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "express": "^4.18.2",
        "fast-glob": "^3.3.2",
        "monaco-editor": "^0.44.0",
        "prettier": "3.1.0",
        "ts-node": "^10.9.1",
        "typescript": "~5.2.2",
        "vite": "~4.4.9",
        "vite-plugin-dts": "^3.6.3",
