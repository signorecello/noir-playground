        "@noir-lang/types": "^0.19.2",
        "fflate": "^0.8.1",
        "js-base64": "^3.7.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-toastify": "^9.1.3",
        "styled-components": "^6.1.1"
    },
    "devDependencies": {
